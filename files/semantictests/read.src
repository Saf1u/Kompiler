function main()=>void{
    localvar x:integer;
    localvar res:integer;
    read(x);
    while(x>-1){
        res=factorial(x);
        write(res);
        read(x);
    };
   

}

function factorial(n:integer)=>integer{
    if (n==0)then{
   return(1);
    }else{
   return(n*factorial(n-1));
    };
}